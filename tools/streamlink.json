{
  "name": "streamlink",
  "description": "CLI utility that pipes video streams from various services into a video player",
  "homepage": "https://streamlink.github.io",
  "install": {
    "windows": {
      "winget": "Streamlink.Streamlink",
      "choco": "streamlink"
    },
    "macos": {
      "brew": "streamlink"
    },
    "linux": {
      "apt": "streamlink",
      "dnf": "streamlink",
      "pacman": "streamlink"
    }
  },
  "verify": "streamlink --version"
}
