{
  "name": "fastfetch",
  "description": "Fast system information tool written in C, similar to neofetch but faster",
  "homepage": "https://github.com/fastfetch-cli/fastfetch",
  "install": {
    "windows": {
      "winget": "Fastfetch-cli.Fastfetch",
      "choco": "fastfetch"
    },
    "macos": {
      "brew": "fastfetch"
    },
    "linux": {
      "apt": "fastfetch",
      "dnf": "fastfetch",
      "pacman": "fastfetch"
    }
  },
  "verify": "fastfetch --version"
}
